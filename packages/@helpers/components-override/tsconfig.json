{
  "include": ["./lib/**/*.*"],
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "module": "esnext",
    "sourceMap": true,
    "outDir": "dist",
    "target": "es5",
    "baseUrl": ".",
    "declarationDir": "dist/@types"
  }
}
